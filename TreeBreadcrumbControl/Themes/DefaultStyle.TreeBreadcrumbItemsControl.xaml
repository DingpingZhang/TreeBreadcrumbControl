<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:treeBreadcrumb="clr-namespace:TreeBreadcrumbControl">

    <Style TargetType="{x:Type treeBreadcrumb:TreeBreadcrumbItemsControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type treeBreadcrumb:TreeBreadcrumbItemsControl}">
                    <treeBreadcrumb:CollapseOverflowItemsPanel x:Name="PART_CollapseOverflowItemsPanel"
                                                               IsItemsHost="True"
                                                               Orientation="Horizontal"
                                                               Reserve="True" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>